    <link rel="stylesheet" href="../css/ui_dark/jquery-ui.css">
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="//code.jquery.com/jquery-1.10.2.js"></script>
    <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
    <script type="text/javascript" src="~/AnythingSlider1.9.2/js/jquery.anythingslider.min.js"></script>    
    <script type="text/javascript">

        $(function () {
            $("#right_nav").accordion();

            $("#slider").anythingSlider({ theme: 'cs-portfolio', autoPlay: true, buildArrows: false, buildNavigation: false, buildStartStop: false });
        })
    </script>
